{
    "root": true,
    "settings": 
    {
      "import/resolver": "node"
    },
    "extends": 
    [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": 
    {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "env": 
    {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": 
    {
      "no-console": 0,
      "import/default": 0
    }
    
  }
  